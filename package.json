{
  "name": "git-it-electron",
  "version": "3.0.0",
  "description": "An open source desktop app for learning Git and GitHub",
  "keywords": [
    "Git",
    "GitHub",
    "Git-it",
    "tutorial",
    "guide",
    "learn",
    "desktop"
  ],
  "productName": "Git-it",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "test": "standard **/*.js",
    "build-chals": "node lib/build-challenges.js",
    "build-pages": "node lib/build-pages.js",
    "build-zhtw": "node lib/build-challenges.js zhtw",
    "build-ja": "node lib/build-challenges.js ja",
    "build-all": "npm run build-chals && npm run build-pages && npm run build-zhtw && npm run build-ja",
    "pack-mac": "electron-packager . Git-it --platform=darwin --arch=x64 --version=0.31.2 --icon=assets/git-it.icns --ignore='assets/PortableGit' --prune=true --out='../Git-it-Packaged-Apps'",
    "pack-win": "electron-packager . Git-it --platform=win32 --arch=ia32   --version=0.31.2 --icon=assets/git-it.ico --prune=true --out='../Git-it-Packaged-Apps'",
    "pack-lin": "electron-packager . Git-it --platform=linux --arch=x64   --version=0.31.2 --icon=assets/git-it.png --ignore='assets/PortableGit' --prune=true --out='../Git-it-Packaged-Apps'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jlord/git-it-electron.git"
  },
  "author": "Jessica Lord",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/jlord/git-it-electron/issues"
  },
  "homepage": "https://github.com/jlord/git-it-electron",
  "devDependencies": {
    "electron-packager": "^5.2.0",
    "electron-prebuilt": "^0.36.0",
    "standard": "^5.4.1"
  },
  "dependencies": {
    "cheerio": "^0.19.0",
    "glob": "^5.0.5",
    "handlebars": "^3.0.3",
    "request": "^2.57.0"
  }
}
